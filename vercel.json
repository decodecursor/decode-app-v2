{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/pay/(.*)",
      "dest": "/pay/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://decode-app-v2.vercel.app",
    "EMAIL_FROM": "DECODE Beauty <noreply@decode.beauty>",
    "SUPPORT_EMAIL": "support@decode.beauty",
    "DEBUG_EMAIL": "false"
  },
  "functions": {
    "app/api/webhooks/crossmint/route.ts": {
      "maxDuration": 30
    },
    "app/api/wallet/create/route.ts": {
      "maxDuration": 30
    }
  }
}